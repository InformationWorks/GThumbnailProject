<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="GThumb">
        <attr name="textSize" format="dimension"/>
        <attr name="previewText" format="string"/>
        <attr name="monoBGColor" format="color"/>
        <attr name="monoTextColor" format="color"/>
        <attr name="backgroundShape" format="enum">
            <enum name="round" value="0" />
            <enum name="square" value="1" />
        </attr>
        <attr name="boldText" format="boolean" />
    </declare-styleable>
</resources>